package com.game.module.farm.model;

public class FarmInfo {
	private int level;  //等级
	private int exp;//升级所需经验
	private int landNum;//拥有农田数
	private int course;//新手教程Id
	private int openFunction;//开放功能
	private String functionIcon;//功能图标
	public int getLevel() {
		return level;
	}
	public void setLevel(int level) {
		this.level = level;
	}
	public int getExp() {
		return exp;
	}
	public void setExp(int exp) {
		this.exp = exp;
	}
	public int getLandNum() {
		return landNum;
	}
	public void setLandNum(int landNum) {
		this.landNum = landNum;
	}
	public int getCourse() {
		return course;
	}
	public void setCourse(int course) {
		this.course = course;
	}
	public int getOpenFunction() {
		return openFunction;
	}
	public void setOpenFunction(int openFunction) {
		this.openFunction = openFunction;
	}
	public String getFunctionIcon() {
		return functionIcon;
	}
	public void setFunctionIcon(String functionIcon) {
		this.functionIcon = functionIcon;
	}
}
